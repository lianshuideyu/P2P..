<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--填充-->
    <solid android:color="#ff9d77"/>

    <!--描边-->
    <stroke android:color="#fad3cf"
        android:width="2dp"/>

    <!--圆角-->
    <corners android:radius="15dp"/>

    <!-- 间隔 -->
    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" /> <!-- 设置各个方向的间隔 -->
</shape>